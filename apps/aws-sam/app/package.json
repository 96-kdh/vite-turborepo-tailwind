      "test": "npm run compile && npm run unit"
   },
   "dependencies": {
      "@aws-sdk/client-dynamodb": "^3.767.0",
      "@aws-sdk/client-sqs": "^3.758.0",
      "@aws-sdk/lib-dynamodb": "^3.767.0",
      "@repo/eslint-config": "file:../../../packages/eslint-config",
      "@repo/hardhat": "file:../../../packages/hardhat",
      "@repo/typescript-config": "file:../../../packages/typescript-config",
      "alchemy-sdk": "^3.5.6",
      "esbuild": "^0.14.54",
      "ethers": "^6.13.5",
      "moment-timezone": "^0.5.48"
   },
   "devDependencies": {
      "@aws-sdk/client-lambda": "^3.774.0",
      "@jest/globals": "^29.2.0",
      "@types/aws-lambda": "^8.10.92",
      "@types/jest": "^29.2.0",
      "@types/node": "^20.5.7",
      "jest": "^29.2.1",
      "ts-jest": "^29.0.5",
      "ts-node": "^10.9.1"
   }
}
