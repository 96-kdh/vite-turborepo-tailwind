{
   "name": "aws-sam",
   "version": "1.0.0",
   "license": "MIT",
   "scripts": {
      "dev": "npm run install:vendor && npm run run:lambda",
      "install:vendor": "mkdir -p ./vendor/@repo/hardhat/script && cp -R ../../../packages/hardhat/script/* ./vendor/@repo/hardhat/script",
